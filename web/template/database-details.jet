{{ extends "soda.jet" }}

{{block head()}}
    <title>{{ .Database.Name }} - database details - Soda</title>
{{end}}

{{block subheader()}}
<h2>{{ .Database.Name }}<span> / database</span></h2>
{{end}}

{{block body()}}

<div>
    <dl>
        <dt>Name</dt>
        <dd>{{ .Database.Name }}</dd>
        <dt>Server</dt>
        <dd><a href="/servers/{{ .Server.Id }}">{{ .Server.Name }}</a></dd>
    </dl>
</div>

{{end}}