    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/soda/main.go",
            // "port": 2345,
            // "host": "127.0.0.1",
            "showLog": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "SODA_SYSTEM_DB_USER": "root",
                "SODA_SYSTEM_DB_PASSWORD": "password",
                "SODA_SYSTEM_DB_ADDR": "127.0.0.1:3306",
                "SODA_SYSTEM_DB_NAME": "soda"
            }
        }
    ]
}